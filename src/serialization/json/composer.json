{
  "name": "ndiritu/sample-serialization-json",
  "description": "Test Implementation of Kiota serialization abstractions using Json.",
  "license": ["MIT"],
  "version": "1.3.0",
  "repositories": [
	{
	  "type": "path",
	  "url": "../../abstractions",
	  "options": {
		"symlink": false
	  }
	}
  ],
  "autoload": {
    "psr-4": {
      "Microsoft\\Kiota\\Serialization\\Json\\": "src"
    }
  },
  "autoload-dev": {
	"psr-4": {
		"Microsoft\\Kiota\\Serialization\\Tests\\": "tests"
	}
  },
  "require": {
    "ndiritu/sample-abstractions": "^1.4.0",
    "guzzlehttp/psr7": "^1.6 || ^2",
    "php": "^7.4 || ^8",
    "ext-json": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^8 || ^9",
    "phpstan/phpstan": "^1.4"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
